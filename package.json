{
  "name": "phosphor-solid",
  "version": "1.1.4",
  "description": "A clean and friendly icon family for Solid-js",
  "author": "rvespula",
  "license": "MIT",
  "repository": "phosphor-solid",
  "homepage": "https://github.com/aguilera51284/phosphor-react",
  "module": "dist/index.esm.js",
  "main": "dist/index.js",
  "sideEffects": false,
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "keywords": [
    "solid",
    "solidjs",
    "icons",
    "svg",
    "phosphor",
    "design",
    "interface",
    "UI",
    "UX"
  ],
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build --format amd",
    "test": "tsdx test --passWithNoTests",
    "lint": "tsdx lint",
    "collate": "node bin/collate.js",
    "assemble": "node bin/assemble.js",
    "format": "prettier --write \"./src/**/*.{js,jsx,ts,tsx,json,vue}\""
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": false,
    "trailingComma": "es5"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^10.4.9",
    "@types/react": "^16.9.43",
    "@types/react-dom": "^16.9.8",
    "chalk": "^4.1.0",
    "husky": "^4.2.5",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "solid-scripts": "0.0.59",
    "tsdx": "^0.14.1"
  },
  "dependencies": {
    "babel-preset-solid": "^1.1.2",
    "solid-js": "^1.1.2"
  }
}
